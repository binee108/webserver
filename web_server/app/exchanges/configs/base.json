{
  "performance": {
    "cache_ttl_seconds": 300,
    "rate_limit_buffer": 0.8,
    "connection_pool_size": 10,
    "request_timeout_seconds": 30,
    "retry_attempts": 3,
    "retry_delay_seconds": 1.0,
    "enable_http_keep_alive": true
  },
  "features": {
    "use_custom_exchange": true,
    "enable_detailed_logging": true,
    "enable_advanced_caching": true,
    "enable_websocket": false,
    "enable_parallel_requests": true,
    "enable_circuit_breaker": true,
    "enable_metrics_collection": true,
    "enable_automatic_failover": false
  },
  "exchanges": {
    "binance": {
      "preferred_implementation": "native",
      "fallback_implementation": "native",
      "rate_limit_mode": "conservative",
      "market_data_source": "primary",
      "order_execution_mode": "safe"
    }
  },
  "security": {
    "encrypt_api_keys": true,
    "api_key_rotation_days": 90,
    "rate_limit_enforcement": true,
    "whitelist_enabled": false,
    "allowed_ips": [],
    "audit_logging": true
  }
}